{
  "buildCommand": "ENV=production pnpm run build",
  "cleanUrls": true,
  "devCommand": "pnpm run dev",
  "framework": null,
  "installCommand": "pnpm install",
  "outputDirectory": "dist",
  "public": true,
  "trailingSlash": false,
  "redirects": [
    { "source": "/quick-start", "destination": "/start.html" },
